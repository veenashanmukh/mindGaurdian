# Example .env for frontend (Vite expects VITE_ prefixed keys)
# Replace values with your Firebase project config if you want Firestore/Analytics.
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Optional Google Analytics measurement id (same as measurement ID above)
VITE_GTAG_MEASUREMENT_ID=
